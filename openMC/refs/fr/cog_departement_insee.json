  "detect": {
    "label_keywords": [
      "code_departement",
      "departement_code",
      "department_code",
      "num_departement",
      "numero_departement",
      "dep_code",
      "cd_dep"
    ],
    "negative_keywords": [
      "nom",
      "libelle",
      "label",
      "intitule",
      "departement_nom",
      "nom_departement"
    ]
  }
}
